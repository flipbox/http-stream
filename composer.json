{
  "name": "flipboxdigital/http-stream",
  "description": "HTTP PSR-7 Stream",
  "version": "2.0.0",
  "keywords": [
    "http",
    "psr-7",
    "stream"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Flipbox Factory",
      "email": "hello@flipboxfactory.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "zendframework/zend-diactoros": "~1.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Flipbox\\Http\\Stream\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Flipbox\\Http\\Stream\\Tests\\": "tests/src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    }
  ]
}